<script>
    $(document).ready(function () {


         $.ajax({
            url: '/Home/OrgName',
            method: 'GET',
            dataType: 'json',
            success: function (data) {
                $('h1#Name').html(data.Name);
            },
            error: function () {
                alert();
            }

        });
            
        $("#dataTables").DataTable({
            responsive: true,
            "language": {
                "search": "Filter records:"
            },
            "ajax": {
                "url": "/Home/GetDashBordData",
                "type": "GET",
                "datatype": "json"
            },
            "columns": [
                { "data": "BuildingID"},
                { "data": "Building_Description"},
                { "data": "Floor"},
                { "data": "Room"},
                { "data": "Room_Description"},
                { "data": "Room_Type"},
                { "data": "AssetName"},
                { "data": "ComponentName"}
            ]
        });


        $.ajax({
            url: '/Home/LoadOverDueInspection',
            method: 'GET',
            dataType: 'json',
            success: function(data) {
                $('p#ODI').html(data.ODI);
            },
            error:function() {
                alert();
            }
            
        });


        $.ajax({
            url: '/Home/LoadOverDueWorkOrder',
            method: 'GET',
            dataType: 'json',
            success: function (data) {
                $('p#ODWO').html(data.ODW);
                
            },
            error: function () {
                alert();
            }

        });

        $.ajax({
            url: '/Home/ActiveInspections',
            method: 'GET',
            dataType: 'json',
            success: function (data) {
                $('p#AI').html(data.ACI);
            },
            error: function () {
                alert();
            }

        });

        $.ajax({
            url: '/Home/ActiveWorkOrders',
            method: 'GET',
            dataType: 'json',
            success: function (data) {
                $('p#AWO').html(data.ACWO);
            },
            error: function () {
                alert();
            }

        });
    });
</script>
       
    
